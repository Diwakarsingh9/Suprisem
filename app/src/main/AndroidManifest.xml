        <activity android:name=".InnerPrductActivity" >
        </activity>
        <activity android:name=".Partnerwithus" >
        </activity>
        <activity android:name=".Payment_and_deliveryActivity" >
        </activity>
        <activity android:name=".AboutActivity" >
        </activity>
        <activity android:name=".CartActivity" >
            </activity>
        <activity android:name=".ContactsActivity" >
        </activity>
        <activity android:name=".Faq_Activity" >
        </activity>


    </application>


</manifest>